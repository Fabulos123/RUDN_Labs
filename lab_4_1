#include <iostream>
using namespace std;
int main() {
    int n;

    // Запрос числа у пользователя
    cout << "Введите число n: ";
    cin >> n;

    // Вывод делителей в порядке убывания
    cout << "Делители числа " << n << " в порядке убывания:" << endl;

    // Ищем делители
    for (int i = n; i >= 1; i--) {
        if (n % i == 0) { // Если i является делителем n
            cout << i << endl; // Выводим делитель
        }
    }

    return 0;
}
